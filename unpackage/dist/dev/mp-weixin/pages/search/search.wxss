.search-search{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	height: 70rpx;
	margin: 70rpx 30rpx 50rpx 30rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background: #f7f7f7;
	border-radius: 50rpx;
}
.search-search text{
	margin: 0 26rpx;
}
.search-search input{
	font-size: 26rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.search-history{
	margin: 0 30rpx 50rpx 30rpx;
	font-size: 26rpx;
}
.search-history-head{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;	
	margin-bottom: 36rpx;
}
.search-history-list{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.search-history-list view{
	padding: 16rpx 28rpx;
	border-radius: 40rpx;
	margin-right: 30rpx;
	margin-bottom: 30rpx;
	background: #f7f7f7;
}
.search-hot{
	margin: 0 30rpx;
}
.search-hot-head{
	margin-bottom: 36rpx;
}
.search-hot-item{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	margin-bottom: 58rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.search-hot-top{
	color: #fb2222;
	width: 60rpx; 
	margin-left: 8rpx;
}
.search-hot-word{
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.search-hot-word view:nth-child(1){
	font-size: 30rpx;
	color: black;
	margin-bottom: 5rpx;
}
.search-hot-word view:nth-child(2){
	font-size: 24rpx;
	color: #878787;
}
.search-hot-word image{
	width: 48rpx;
	height: 22rpx;
}
.search-hot-count{
	color: #878787;
	font-size: 26rpx;
}
.search-result{
	border-top: 2rpx #e4e4e4 solid;
	padding: 30rpx;
}
.search-result-item{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-bottom: 30rpx;
	margin-bottom: 30rpx;
	border-bottom: 2rpx #e4e4e4 solid;
}
.search-result-word{}
.search-result-word view:nth-child(1){
	font-size: 28rpx;
	color: #235790;
	margin-bottom: 12rpx
}
.search-result-word view:nth-child(2){
	font-size: 22rpx;
	color: #898989
}
.search-result-item text{
	font-size: 50rpx;
}
.search-suggest{
	border-top: 2rpx #e4e4e4 solid;
	padding: 30rpx;
	font-size: 26rpx;
}
.search-suggest-head{
	color: #4574a5;
	margin-bottom: 74rpx
}
.search-suggest-item{
	color: #5d5d5d;
	margin-bottom: 74rpx
}
.search-suggest-item text{
	color: #bdbdbd;
	margin-right: 28rpx;
	position: relative;
	top: 2rpx
}

